{
  "name": "factorio-src-bot",
  "private": true,
  "main": "out/main.js",
  "scripts": {
    "build": "npm run clean && tsc -p tsconfig-release.json",
    "clean": "rm -rf out",
    "dev": "NODE_ENV=development node --watch out/main.js",
    "watch": "tsc -w",
    "lint": "eslint && prettier --check .",
    "lint:fix": "eslint --fix && prettier --write .",
    "check": "npm run lint && tsc --noEmit",
    "migrate": "sequelize-cli db:migrate",
    "migrate:undo": "sequelize-cli db:migrate:undo",
    "migrate:undo:all": "sequelize-cli db:migrate:undo:all",
    "migrate:status": "sequelize-cli db:migrate:status"
  },
  "type": "module",
  "dependencies": {
    "@sapphire/decorators": "^6.2.0",
    "@sapphire/framework": "^5.4.0",
    "@sapphire/plugin-logger": "^4.1.0",
    "@sapphire/plugin-subcommands": "^7.0.1",
    "discord.js": "^14.25.1",
    "dotenv": "^17.2.3",
    "iso8601-duration": "^2.1.3",
    "node-schedule": "^2.1.1",
    "reflect-metadata": "^0.2.2",
    "sequelize": "^6.37.7",
    "sequelize-typescript": "^2.1.6",
    "sqlite3": "^5.1.7",
    "src-ts": "^2.10.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node-schedule": "^2.1.8",
    "discord-api-types": "^0.38.36",
    "eslint": "^9.39.1",
    "prettier": "^3.7.4",
    "sequelize-cli": "^6.6.3",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.49.0"
  },
  "prettier": {
    "semi": false,
    "printWidth": 120
  }
}
